{
  "kind": "collectionType",
  "collectionName": "public_users",
  "info": {
    "name": "public-user",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "show_phone": {
      "type": "boolean"
    },
    "state": {
      "type": "boolean",
      "default": true
    },
    "phone": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "surname": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "id_private": {
      "type": "biginteger",
      "required": false
    },
    "profile": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos",
        "files"
      ],
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "categories"
    },
    "publications": {
      "collection": "publications",
      "via": "public_user"
    },
    "verified": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "blocked": {
      "type": "boolean"
    },
    "country": {
      "type": "string",
      "default": "Argentina"
    },
    "province": {
      "type": "string",
      "default": "Chaco"
    },
    "city": {
      "type": "string"
    },
    "location": {
      "model": "location",
      "via": "public_users"
    }
  }
}
